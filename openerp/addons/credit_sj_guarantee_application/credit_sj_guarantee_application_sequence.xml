<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- 核保单据编号 -->
        <record id="seq_credit_sj_guarantee_application" model="ir.sequence.type">
            <field name="name">核保序列编号</field>
            <field name="code">credit.sj.guarantee.application</field>
        </record>
        <record id="seq_credit_sj_guarantee_application_number" model="ir.sequence">
            <field name="name">核保编号</field>
            <field name="code">credit.sj.guarantee.application</field>
            <field name="prefix">H</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- 核保路线编号 -->
        <record id="seq_credit_sj_guarantee_route" model="ir.sequence.type">
            <field name="name">核保路线序列编号</field>
            <field name="code">credit.sj.guarantee.route</field>
        </record>
        <record id="seq_credit_sj_guarantee_route_number" model="ir.sequence">
            <field name="name">核保路线编号</field>
            <field name="code">credit.sj.guarantee.route</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
